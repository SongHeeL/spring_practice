<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.spring_practice.mapper.ProductMapper">
    <insert id="addProduct" parameterType="com.example.spring_practice.dao.ProductDao">
        insert into product (name, info)
        values (#{name}, #{info})
    </insert>
</mapper>